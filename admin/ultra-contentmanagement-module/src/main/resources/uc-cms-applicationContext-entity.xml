<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    
    <!-- Entity mappings - override for extensibility -->
    <bean id="com.ultracommerce.cms.page.domain.Page" class="com.ultracommerce.cms.page.domain.PageImpl" scope="prototype"/>
    <bean id="com.ultracommerce.cms.page.domain.PageAttribute" class="com.ultracommerce.cms.page.domain.PageAttributeImpl" scope="prototype"/>
    <bean id="com.ultracommerce.cms.page.domain.PageRule" class="com.ultracommerce.cms.page.domain.PageRuleImpl" scope="prototype"/>

    <bean id="com.ultracommerce.cms.page.domain.PageTemplate" class="com.ultracommerce.cms.page.domain.PageTemplateImpl" scope="prototype"/>
    <bean id="com.ultracommerce.cms.file.domain.StaticAssetStorage" class="com.ultracommerce.cms.file.domain.StaticAssetStorageImpl" scope="prototype"/>
    <bean id="com.ultracommerce.cms.file.domain.StaticAsset" class="com.ultracommerce.cms.file.domain.StaticAssetImpl" scope="prototype"/>
    <bean id="com.ultracommerce.cms.structure.domain.StructuredContent" class="com.ultracommerce.cms.structure.domain.StructuredContentImpl" scope="prototype"/>
    <bean id="com.ultracommerce.cms.structure.domain.StructuredContentType" class="com.ultracommerce.cms.structure.domain.StructuredContentTypeImpl" scope="prototype"/>
    <bean id="com.ultracommerce.cms.structure.domain.StructuredContentFieldTemplate" class="com.ultracommerce.cms.structure.domain.StructuredContentFieldTemplateImpl" scope="prototype"/>
    <bean id="com.ultracommerce.cms.structure.domain.StructuredContentRule" class="com.ultracommerce.cms.structure.domain.StructuredContentRuleImpl" scope="prototype"/>
    <bean id="com.ultracommerce.cms.url.domain.URLHandler" class="com.ultracommerce.cms.url.domain.URLHandlerImpl" scope="prototype"/>

    <bean id="com.ultracommerce.cms.field.domain.FieldDefinition" class="com.ultracommerce.cms.field.domain.FieldDefinitionImpl" scope="prototype"/>
    <bean id="com.ultracommerce.cms.structure.domain.StructuredContentField" class="com.ultracommerce.cms.structure.domain.StructuredContentFieldImpl" scope="prototype"/>

</beans>
