<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm http://java.sun.com/xml/ns/persistence/orm_2_0.xsd" version="2.0">

    <named-query name="UC_READ_FIRST_SHIPPING_RATE_BY_FEE_TYPES" >
        <query>SELECT shippingRate FROM com.ultracommerce.core.pricing.domain.ShippingRate shippingRate
                     WHERE shippingRate.feeType = :feeType
                           AND shippingRate.feeSubType = :feeSubType 
                           AND shippingRate.bandUnitQuantity &gt; :bandUnitQuantity 
                 ORDER BY band_unit_qty         
        </query>
    </named-query>
    
</entity-mappings>
